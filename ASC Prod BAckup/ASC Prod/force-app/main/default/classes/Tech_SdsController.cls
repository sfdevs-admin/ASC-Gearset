    @AuraEnabled(cacheable=true)
    public static Product2 getProductRecord(String sku) {
        try{
            // Product2 prodValue = [SELECT Id, Technical_Notes_URL__c FROM Product2 WHERE StockKeepingUnit = :sku LIMIT 1];
            // system.debug('cart'+prodValue);
            // return prodValue;

            Product2 prodValue;
            List<Product2> prodList = [SELECT Id, Technical_Notes_URL__c, SDS_URL_CH__c, SDS_URL_CN__c, SDS_URL_FR__c, SDS_URL_GB__c, SDS_URL_KR__c, SDS_URL_US__c FROM Product2 WHERE StockKeepingUnit = :sku LIMIT 1];
            system.debug('prodList'+prodList);
            if( ! prodList.isEmpty() ){
                prodValue = prodList[0];
            }
            system.debug('prodValue'+prodValue);
            return prodValue;
        }
        catch(Exception E){
            //throw new AuraHandledException(CLASS_NAME + ' - ' + METHOD_NAME + ' - Error line ' + objEx.getLineNumber() + ' - Type ' + objEx.getTypeName() + ' - Message  ' + objEx.getMessage());
            System.debug('Error: ' + E.getMessage());
            return null; // or any default value
        }
    }
    
