public class ContentVersionTriggerHandler {
    public static void processTechnicalDocuments(List<ContentVersion> newList){
        Map<String,String> productCodeMap = new Map<String,String>();
        for(ContentVersion cv : newList){
            if(cv.PathOnClient != '' && cv.PathOnClient.containsIgnoreCase('Technical') && cv.Pathonclient.contains('\\')){
                String pCode = cv.PathOnClient.right(15);
                pCode = pCode.left(7);
                productCodeMap.put(pCode,cv.ContentDocumentId+'-'+cv.Id);
                System.debug(productCodeMap);
            }
        }
        List<ContentDocumentLink> cdlToInsert = new List<ContentDocumentLink>();
        Set<Id> cvIdSet = new Set<Id>();
        Map<Product2,ContentDistribution> cdToInsert = new Map<Product2,ContentDistribution>();
        for(Product2 productInstance : [SELECT id, StockKeepingUnit, Technical_Notes_URL__c FROM Product2 WHERE StockKeepingUnit IN: productCodeMap.keySet()]){
            ContentDocumentLink cdl = new ContentDocumentLink();
            cdl.ContentDocumentId = productCodeMap.get(productInstance.StockKeepingUnit).split('-')[0];
            cdl.LinkedEntityId = productInstance.Id;
            cdlToInsert.add(cdl);
            ContentDistribution cd = new ContentDistribution();
            cd.ContentVersionId = productCodeMap.get(productInstance.StockKeepingUnit).split('-')[1];
            cd.Name = productCodeMap.get(productInstance.StockKeepingUnit).split('-')[1];
            cd.PreferencesAllowViewInBrowser = true;
            cd.PreferencesLinkLatestVersion = true;
            cVidSet.add(productCodeMap.get(productInstance.StockKeepingUnit).split('-')[1]);
            cdToInsert.put(productInstance,cd);
        }
        insert cdlToInsert;
        insert cdToInsert.values();
        List<Product2> productsToUpdate = new List<Product2>();
        List<ContentDistribution> cdListNew = [SELECT Id, DistributionPublicURL, ContentVersionId FROM ContentDistribution WHERE ContentVersionId IN: cvIdSet];
        for(Product2 p : cdToInsert.keySet()){
            for(ContentDistribution cd : cdListNew ){
                if(cdToInsert.containskey(p) && cdToInsert.get(p) != null && cdToInsert.get(p).ContentVersionId == cd.ContentVersionId){
                    p.Technical_Notes_URL__c = cd.DistributionPublicURL;
                    productsToUpdate .add(p);
                }
            }
        }
        update productsToUpdate ;
        //processTechnicalNotesURL(JSON.serialize(cdToInsert.values()));
    }
    
    @future
    public static void processTechnicalNotesURL(String contentDistributionRecords){
        List<ContentDistribution> cdListNew = (List<ContentDistribution>) JSON.deserialize(contentDistributionRecords,List<ContentDistribution>.class);
        List<Product2> productsToUpdate = new List<Product2>();
        Map<Id,Id> cvcdMap = new Map<Id,Id>();
        for(ContentDistribution c : cdListNew ){
            cvcdMap.put(c.contentversionid,c.contentdocumentId);
        }
        Map<Id,Id> newMap = new Map<Id,Id>();
        Set<Id> filterSet = new Set<Id>();
        for(Id i : cvcdMap.values()){
            filterSet.add(i);
        }
        List<ContentDocumentLink> cdlist = [SELECT Id, LinkedEntityId, ContentDOcumentId  FROM ContentDocumentLink WHERE ContentDocumentId IN: filterSet ];
        for(ContentDocumentLink cdl : cdlist ){
            //if(cdl.linkedentityid.getSObjectType().getDescribe().getName() == 'Product2'){
                newMap.put(cdl.contentdocumentId, cdl.linkedentityid);
            //}
        }
        List<ContentDistribution> finalCDs = [SELECT id, DistributionPublicURL, ContentDocumentId FROM ContentDistribution WHERE ContentVersionId IN: cvcdMap.keySet()];
        Map<Id,String> finalMap = new Map<Id,String>();
        for(ContentDistribution c : finalCDs){
            if(newMap.containsKey(c.ContentDocumentId)){
                finalMap.put(newMap.get(c.ContentDocumentId),c.DistributionPublicURL);
            }
        }
        for(Product2 p : [SELECT Id, Technical_Notes_URL__c FROM Product2 WHERE ID IN: finalMap.keySet()]){
            p.Technical_Notes_URL__c = finalMap.get(p.Id);
            productsToUpdate.add(p);
        }
        update productsToUpdate;
    }
    
    public static void processSds(List<ContentVersion> cvList){
        Map<String,List<String>> productSKUMap = new Map<String,List<String>>();
        Set<String> productSKUandLang = new Set<String>();
        Map<String,String> productCodeMap = new Map<String,String>();
        List<ContentDocumentLink> cdlToInsert = new List<ContentDocumentLink>();
        Map<Product2,ContentDistribution> cdToInsert = new Map<Product2,ContentDistribution>();
        Set<Id> cvIdSet = new Set<Id>();
        Map<String,String> productSKULangCvIdMap = new Map<String,String>();
        Map<String,String> productSKULangCdIdMap = new Map<String,String>();
        Map<Id,ContentDistribution> cvContentDistriMap = new Map<Id,ContentDistribution>();
        
        for(ContentVersion cv : cvList){
            if(cv.PathOnClient != '' && cv.PathOnClient.containsIgnoreCase('ssds') && cv.Pathonclient.contains('\\')){
                    //List<String> str = 
                    //Integer strSize = str.size();
                    String s = cv.PathOnClient.substringAfter('SSDS\\');
                    String productWithLanguageAndExt = s.substring(2,s.length());
                    String productWithLanguage = productWithLanguageAndExt.substring(0,productWithLanguageAndExt.length()-4);
                    String sku = productWithLanguage.subString(0,productWithLanguage.length()-3);
                    
                    if(!productSKUMap.containsKey(sku)){
                        productSKUMap.put(sku,new List<String>());
                    }
                    productSKUMap.get(sku).add(productWithLanguage);
                    
                    productSKULangCvIdMap.put(productWithLanguage,cv.Id);
                    
                    productSKULangCdIdMap.put(productWithLanguage,cv.ContentDocumentId);
            }
        }
        system.debug('---'+productSKULangCvIdMap);
        system.debug('---'+productSKULangCdIdMap);
        List<ContentDistribution> cInsert = new List<ContentDistribution>();
        for(Product2 prod : [SELECT Id, SDS_URL_CH__c, SDS_URL_CN__c, SDS_URL_FR__c, SDS_URL_GB__c, SDS_URL_KR__c, SDS_URL_US__c, StockKeepingUnit  
                                FROM Product2 
                                WHERE StockKeepingUnit IN: productSKUMap.keySet()]){
            for(String sku : productSKULangCvIdMap.keySet()){
                String tempSku = sku.subString(0,sku.length()-3);
                if(tempSku == prod.StockKeepingUnit){
                    String st = sku.subString(0,sku.length()-4);
                    ContentDocumentLink cdl = new ContentDocumentLink();
                    cdl.ContentDocumentId = productSKULangCdIdMap.get(sku);
                    cdl.LinkedEntityId = prod.Id;
                    cdlToInsert.add(cdl);
                    
                    ContentDistribution cd = new ContentDistribution();
                    cd.ContentVersionId = productSKULangCvIdMap.get(sku);
                    cd.Name = productSKULangCvIdMap.get(sku);
                    cd.PreferencesAllowViewInBrowser = true;
                    cd.PreferencesLinkLatestVersion = true;
                    cVidSet.add(productSKULangCvIdMap.get(sku));
                    cdToInsert.put(prod,cd);
                    cInsert.add(cd);
                }
            }               
            
        }
        
        insert cdlToInsert;
        insert cInsert;
        
        for(ContentDistribution distribution : [SELECT Id, DistributionPublicURL, ContentVersionId 
                                                FROM ContentDistribution 
                                                WHERE ContentVersionId IN: cvIdSet]){
            cvContentDistriMap.put(distribution.ContentVersionId,distribution);
        }
        
        List<Product2> prodToUpdate = new List<Product2>();
        for(Product2 p : [SELECT Id, SDS_URL_CH__c, SDS_URL_CN__c, SDS_URL_FR__c, SDS_URL_GB__c, SDS_URL_KR__c, SDS_URL_US__c, StockKeepingUnit  
                                FROM Product2 
                                WHERE StockKeepingUnit IN: productSKUMap.keySet()]){
            for(String sku : productSKULangCvIdMap.keySet() ){
                String tempSku = sku.subString(0,sku.length()-3);
                if(tempSku == p.StockKeepingUnit){
                    if(sku.endsWith('CH')){
                        p.SDS_URL_CH__c = cvContentDistriMap.get(productSKULangCvIdMap.get(sku)).DistributionPublicURL;
                        //prodToUpdate.add(p);
                    }
                    else if(sku.endsWith('FR')){
                        p.SDS_URL_FR__c = cvContentDistriMap.get(productSKULangCvIdMap.get(sku)).DistributionPublicURL;
                        //prodToUpdate.add(p);
                    }
                    else if(sku.endsWith('US')){
                        p.SDS_URL_US__c = cvContentDistriMap.get(productSKULangCvIdMap.get(sku)).DistributionPublicURL;
                        
                    }
                    else if(sku.endsWith('KR')){
                        p.SDS_URL_KR__c = cvContentDistriMap.get(productSKULangCvIdMap.get(sku)).DistributionPublicURL;
                        //prodToUpdate.add(p);
                    }
                    else if(sku.endsWith('GB')){
                        p.SDS_URL_GB__c = cvContentDistriMap.get(productSKULangCvIdMap.get(sku)).DistributionPublicURL;
                        //prodToUpdate.add(p);
                    }
                    else if(sku.endsWith('DE')){
                        p.SDS_URL_DE__c = cvContentDistriMap.get(productSKULangCvIdMap.get(sku)).DistributionPublicURL;
                        //prodToUpdate.add(p);
                    }
                    else if(sku.endsWith('CN')){
                        p.SDS_URL_CN__c = cvContentDistriMap.get(productSKULangCvIdMap.get(sku)).DistributionPublicURL;
                        //prodToUpdate.add(p);
                    }
                }
                
            }
            prodToUpdate.add(p);
        }
        update prodToUpdate;
    }
    
