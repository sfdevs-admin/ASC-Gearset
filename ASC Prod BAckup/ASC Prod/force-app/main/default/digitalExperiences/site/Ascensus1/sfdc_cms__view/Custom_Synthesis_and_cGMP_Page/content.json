            "sectionConfig" : "{\"UUID\":\"ce8e6262-ff3b-4dce-95f3-aaaef5fa7ea9\",\"columns\":[{\"UUID\":\"bfa73f72-88ee-4cfa-92e6-be7fcd0340ba\",\"columnName\":\"Column 1\",\"columnKey\":\"col1\",\"columnWidth\":\"6\",\"seedComponents\":[]},{\"UUID\":\"3c879b50-8c48-4a17-8116-e5e98b646c5b\",\"columnName\":\"Column 2\",\"columnKey\":\"8395f\",\"columnWidth\":\"6\",\"seedComponents\":[]}]}"
